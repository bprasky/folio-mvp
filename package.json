{
  "name": "folio-mvp",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "dev:safe": "powershell -Command \"Remove-Item -Recurse -Force .next -ErrorAction SilentlyContinue; next dev\"",
    "dev:protected": "node src/protect-changes.js && npm run clear-cache && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clear-cache": "powershell -Command \"Remove-Item -Recurse -Force .next -ErrorAction SilentlyContinue\"",
    "reset-files": "powershell -Command \"Get-ChildItem -Recurse -Filter '*.backup' | Remove-Item -Force\"",
    "protect": "node src/protect-changes.js",
    "restore": "node src/protect-changes.js restore",
    "setup-db": "node src/scripts/setup-database.js",
    "migrate-data": "node src/scripts/migrate-data.js",
    "test-roles": "node src/scripts/test-role-assignment.js",
    "setup-nextauth": "node src/scripts/setup-nextauth-env.js",
    "copy-env": "node src/scripts/copy-env.js",
    "clean-layout": "node src/scripts/remove-navigation-imports.js",
    "verify-layout": "node src/scripts/verify-layout-cleanup.js",
    "final-cleanup": "node src/scripts/final-layout-cleanup.js",
    "comprehensive-cleanup": "node src/scripts/comprehensive-layout-cleanup.js",
    "targeted-cleanup": "node src/scripts/targeted-layout-cleanup.js",
    "manual-fix": "node src/scripts/manual-layout-fix.js",
    "prisma:seed:nycx": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/seed-nycx.ts",
    "auth:detect": "tsx scripts/auth-detect.ts",
    "auth:detect:simple": "node scripts/auth-detect-simple.js",
    "db:check": "tsx scripts/db-check.ts",
    "test:auth": "node scripts/test-auth-endpoints.js",
    "db:guard": "tsx scripts/guard-migrations.ts",
    "db:backup": "tsx scripts/backup-db.ts",
    "migrate:safe": "npm run db:guard && npm run db:backup && prisma migrate dev",
    "migrate:create-only": "npm run db:guard && prisma migrate dev --create-only",
    "migrate:deploy": "npm run db:guard && prisma migrate deploy",
    "seed:min": "tsx scripts/seed-minimal.ts",
    "check-env": "tsx scripts/check-env.ts",
    "test:service-key": "node scripts/test-supabase-service-key.mjs",
    "guard:client-imports": "node scripts/guard-client-imports.js",
    "diag": "node scripts/diag.mjs",
    "audit:events": "node scripts/audit-events.mjs",
    "bucket:list": "node scripts/list-buckets.mjs",
    "bucket:create": "node scripts/create-storage-bucket.mjs",
    "seed": "prisma db seed",
    "migrate:create": "prisma migrate dev --create-only",
    "schema:push": "prisma generate && prisma db push",
    "db:sync-and-seed": "npm run schema:push && npm run seed"
  },
  "buildTimestamp": "2025-01-18T20:15:00Z",
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  },
